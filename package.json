{
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "baseline-browser-mapping": "^2.8.32",
    "css-loader": "^7.1.2",
    "electron": "^38.2.0",
    "electron-builder": "^26.0.12",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.4.0",
    "postcss-loader": "^8.2.0",
    "standard": "^17.1.2",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.0.0",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  },
  "name": "facturacionsys",
  "version": "1.0.0",
  "main": "main.js",
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "camelcase": [
        "error",
        {
          "allow": [
            "correo_electronico",
            "nombre_completo",
            "precio_costo",
            "precio_venta",
            "image_url",
            "rol_id",
            "factura_id",
            "usuario_id",
            "total_usd",
            "fecha_factura",
            "usuario_nombre",
            "cantidad",
            "precio_unitario_usd",
            "subtotal_usd",
            "producto_nombre",
            "producto_barcode",
            "id_categoria",
            "nombre_categoria"
          ]
        }
      ],
      "quotes": "off",
      "jsx-quotes": "off",
      "no-extra-semi": "off"
    }
  },
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "build": "webpack",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "dropbox": "^10.34.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-datepicker": "^8.9.0",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3",
    "recharts": "^3.2.1",
    "sqlite3": "^5.1.7"
  },
  "build": {
    "appId": "com.daihasys.facturacion",
    "productName": "Catire VRS",
    "directories": {
      "output": "installer"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "main.js",
      "preload.js",
      "server.js",
      "index.html",
      "dist/**/*",
      "src/print.css",
      "database/**/*",
      "utils/**/*",
      "node_modules/**/*",
      "!node_modules/**/test/**",
      "!node_modules/**/*.md"
    ],
    "extraResources": [
      {
        "from": ".env",
        "to": ".env"
      }
    ],
    "asarUnpack": [
      "database/**/*",
      "node_modules/sqlite3/**/*",
      "node_modules/bcrypt/**/*"
    ]
  }
}